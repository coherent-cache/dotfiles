{
    "global": { "show_in_menu_bar": false },
    "profiles": [
        {
            "complex_modifications": {
                "rules": [
                    {
                        "description": "Hyper Key (Caps Lock)",
                        "manipulators": [
                            {
                                "description": "Change caps_lock to command+control+option+shift (Hyper), tap for escape",
                                "from": {
                                    "key_code": "caps_lock",
                                    "modifiers": { "optional": ["any"] }
                                },
                                "to": [
                                    {
                                        "key_code": "left_shift",
                                        "modifiers": ["left_command", "left_control", "left_option"]
                                    }
                                ],
                                "to_if_alone": [{ "key_code": "escape" }],
                                "type": "basic"
                            }
                        ]
                    },
                    {
                        "description": "Hyper+Q: Open Microsoft Teams (Comms)",
                        "manipulators": [
                            {
                                "from": {
                                    "key_code": "q",
                                    "modifiers": {
                                        "mandatory": ["left_shift", "left_command", "left_control", "left_option"],
                                        "optional": ["any"]
                                    }
                                },
                                "to": [{ "shell_command": "open -a 'Microsoft Teams.app'" }],
                                "type": "basic"
                            }
                        ]
                    },
                    {
                        "description": "Hyper+W: Open Firefox (Work)",
